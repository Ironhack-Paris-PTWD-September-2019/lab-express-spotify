<div class="artistCard">
    {{#if images}}
        {{#each images}}
            {{#if @first}}
                <img src="{{url}}" alt="Cover">
            {{/if}}
        {{/each}}
    {{else}}
        <img src="/images/default-cover.png" alt="default cover">
    {{/if}}   
    <h2>{{name}}</h2>
    <a href="/albums/{{id}}">View albums</a>
</div>